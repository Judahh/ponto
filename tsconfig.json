{
    "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "lib": ["ES2021"],
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true
    },
    "include": ["api/**/*.ts"]
  }